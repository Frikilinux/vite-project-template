{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:react/recommended",
    "standard",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-refresh"],
  "settings": { "react": { "version": "18.2" } },
  "rules": {
    "react-refresh/only-export-components": "warn",
    "no-unused-vars": "warn"
  }
}
